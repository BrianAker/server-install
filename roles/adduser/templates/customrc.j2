#!/bin/bash
# {{ ansible_managed }}
export EDITOR=vim

if [ -x /usr/bin/keychain ]; then
  /usr/bin/keychain --quiet
fi

if [ -d /usr/local/bin ]; then
  for x in "/usr/local/bin"; do
    case ":$PATH:" in
      *":$x:"*) :;; # already there
      *) PATH="$x:$PATH";;
    esac
  done
fi

if [ -d /usr/local/sbin ]; then
  for x in "/usr/local/sbin"; do
    case ":$PATH:" in
      *":$x:"*) :;; # already there
      *) PATH="$x:$PATH";;
    esac
  done
fi

if [ -f "$HOME/.perlrc" ]; then
  . $HOME/.perlrc
fi

if [ -f "$HOME/.pythonrc" ]; then
  . $HOME/.pythonrc
fi

if [ -f "$HOME/.rubyrc" ]; then
  . $HOME/.rubyrc
fi
