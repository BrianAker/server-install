# {{ ansible_managed }}
# Source global definitions
if [ -x $HOME/.python/bin/activate ]; then
  . $HOME/.python/bin/activate
elif [ -d $HOME/.python/bin ]; then
  for x in "$HOME/.python/bin"; do
    case ":$PATH:" in
      *":$x:"*) :;; # already there
      *) PATH="$x:$PATH";;
    esac
  done
fi
