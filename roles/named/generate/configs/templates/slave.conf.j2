// vim:ft=htmljinja
// Generated by Ansible

{% for zone in zones %}
{% if zone.type == 'master' %}
zone "{{ zone.name }}" {
  type slave;
  file "slave/{{ zone.name }}";

  masters {
{% if masters %}
{% for host_ in masters %}
    {{ host_ }};
{% endfor %}
{% else %}
    none;
{% endif %}
  };
};

{% endif %}
{% endfor %}
